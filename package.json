{
  "name": "twittfluence",
  "version": "1.0.0",
  "description": "Project for crawling, categorizing and analyzing tweets by their sentiments.",
  "repository": "https://github.com/sebenns/Tweetfluence",
  "main": "start.js",
  "scripts": {
    "compile": "tsc -p ./",
    "prestart": "npm run compile",
    "postinstall": "npm run compile",
    "start": "node ./src/start.js",
    "sstart": "node ./src/start.js --skip",
    "crawl": "node ./src/start.js --crawl",
    "scrawl": "node ./src/start.js --crawl --skip",
    "analyze": "node ./src/start.js --analyze",
    "sanalyze": "node ./src/start.js --analyze --skip",
    "import": "node ./src/start.js --neo4j",
    "simport": "node ./src/start.js --skip",
    "docker": "docker-compose up --build"
  },
  "author": "Sebastian Enns",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.0.2",
    "@types/node-fetch": "^2.5.11",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@google-cloud/language": "^4.2.6",
    "neo4j-driver": "^4.3.1",
    "node-fetch": "^2.6.1"
  }
}
